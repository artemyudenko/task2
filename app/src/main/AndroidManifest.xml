<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.artemyudenko.task2">

    <permission-group
        android:name="com.permissions"
        android:label="my permissions group" />

    <permission
        android:name="com.permissions.NOTI_CLICK"
        android:label="my permission for receive intents"
        android:permissionGroup="com.permissions"
        android:protectionLevel="normal" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="com.artemyudenko.my_permissions.SHARE_INTENT" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>